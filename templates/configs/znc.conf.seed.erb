<User <%= @name %>>
        Nick       = <%= @nickname %>
        AltNick    = <%= @altnick %>
        Ident      = <%= @ident %>
        RealName   = <%= @realname %>
        QuitMsg = '<%= @quitmsg %>'
        ChanModes  = +stn
        Buffer     = <%= @buffer %>
        KeepBuffer = <%= @keepbuffer %>
        
        MultiClients = true
        BounceDCCs = true
        Admin = <%= @admin %>
        DCCLookupMethod = default
        TimestampFormat = [%H:%M:%S]
        TimezoneOffset = 0.00
        JoinTries = 10
        MaxJoins = 5
        Allow = *
        AutoClearChanBuffer = true
        DenyLoadMod = true
        DenySetBindHost = true
        MaxNetworks = <%= @maxnetworks %>
        PrependTimestamp = true
        AppendTimestamp = false
        StatusPrefix = *
        Timezone = <%= @timezone %>
        
        <%- if @pass_hash %>
        <Pass password>
            Hash = <%= @pass_hash['hash'] %>
            Method = <%= @pass_hash['method'].upcase %>
            Salt = <%= @pass_hash['salt'] %>
        </Pass>
        <%- end %>
